<h1>All Restaurants</h1>
<% @restaurants.each do |restaurant|%>
  <h3><%= restaurant.name %> <%= '⭐️' * restaurant.rating %></h3>
  <p><%= restaurant.address %></p>
<% end %>

<ul>
  <% @restaurants.each do |restaurant| %>
    <li><%= link_to restaurant.name, restaurant_path(restaurant) %></li>
  <% end %>
</ul>
